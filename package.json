{
  "name": "flair-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate-routes": "tsr generate",
    "dev": "tsr generate && vite",
    "build": "tsr generate && tsc -b && vite build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:check": "eslint \"{src,test}/**/*.ts\" --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --port 5173",
    "db:seed:e2e": "docker compose -f docker-compose.e2e.yml exec -T api node ./dist/scripts/seed.js",
    "test:e2e": "npx playwright test",
    "test:e2e:local": "npm run docker:test:down && npm run docker:test:up && npm run db:seed:e2e && npx playwright test --ui",
    "docker:test:up": "docker compose -f docker-compose.e2e.yml pull api && docker compose -f docker-compose.e2e.yml up -d",
    "docker:test:down": "docker compose -f docker-compose.e2e.yml down -v --remove-orphans"
  },
  "dependencies": {
